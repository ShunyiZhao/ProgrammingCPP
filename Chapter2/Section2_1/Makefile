TEST2_3SOURCE = practice2_3
TEST2_4SOURCE = practice2_4
TEST2_5SOURCE = practice2_5

STD = -std=c++14
CC = clang++

runTest2_3:
	$(CC) $(STD) $(TEST2_3SOURCE).cpp -o $(TEST2_3SOURCE)
	./$(TEST2_3SOURCE)

runTest2_4:
	$(CC) $(STD) $(TEST2_4SOURCE).cpp -o $(TEST2_4SOURCE)
	./$(TEST2_4SOURCE)

runTest2_5:
	$(CC) $(STD) $(TEST2_5SOURCE).cpp -o $(TEST2_5SOURCE)
	./$(TEST2_5SOURCE)